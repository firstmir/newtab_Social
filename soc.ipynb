    "import webbrowser\n",
    "import time\n",
    "\n",
    "\n",
    "# what's new\n",
    "links = [\n",
    "    'https://www.facebook.com/',\n",
    "    'https://twitter.com/',\n",
    "    'https://www.linkedin.com',\n",
    "    'https://www.tiktok.com/en/'\n",
    "    ]\n",
    "\n",
    "for i in links:\n",
    "    webbrowser.open_new_tab(i)\n",
    "    # give time to load the page\n",
    "    time.sleep(5)"
   ]
  }
